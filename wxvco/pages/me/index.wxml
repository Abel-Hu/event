<view>
    <view bindtap="bindViewTap" class="userinfo">
        <image class="userinfo-avatar" src="{{props.member.member.avatarUrl}}" background-size="cover"></image>
        <text class="userinfo-nickname">{{props.member.member.nickName}}</text>
    </view>
    <navigator url="/pages/setting/index">
        <image class="setting" src="/src/img/me/set.png"/>
    </navigator>
    <view class="box">
        <view class="item">
            <navigator url="/pages/event/index">我的参与</navigator>
        </view>
        <view class="item">
            <navigator url="/pages/event/index">我的收藏</navigator>
        </view>
    </view>
    <view class="box">
        <view class="item">
            <navigator url="/pages/event/create/index">发布活动</navigator>
        </view>
    </view>
    <view class="usermotto">
        <text class="user-motto">{{motto}}</text>
        <view>
            {{props.todo.todoText}}
        </view>
    </view>
    <view>
        {{props.member.ns}}
    </view>
</view>